{\s17\sa60\sb30\widctlpar\qj \fs22\cgrid 
{
\par
\pard\plain \s141\fi-360\li720\widctlpar\fs20\cgrid 
1.\tab Web Scraping site de vagas de emprego para busca: {\f2 https://www.vagas.com.br/} \par}
}{
\pard\plain \s41\li360\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid 50 {\cf17 def }busca_site_vagas(pag1, cargos, nr_pag=2):\par
51 \par
52     {\cf20 # Abrindo a p\'E1gina selecionada no navegador.}\par
53     chrome_options = Options()\par
54     chrome_options.add_argument({\cf22 '--headless'})\par
55     \par
56     nav = webdriver.Chrome(options=chrome_options)\par
57     nav.set_page_load_timeout(10)\par
58     nav.get(pag1)\par
59 \par
60     {\cf20 # preenche com o nome do cargo indicado, no campo para pesquisa.}\par
61     nav.find_element(By.XPATH,{\cf22 '/html/body/header/section/div/form/input'}).send_keys(cargos)\par
62     nav.find_element(By.XPATH,{\cf22 '/html/body/header/section/div/form/input'}).send_keys(Keys.ENTER)\par
63 \par
64     {\cf20 # Tempo de espera de 20 segundos para conclus\'E3o ou at\'E9 mostrar todas classes 'cargo' do html.}\par
65     wait = WebDriverWait (nav, 20)\par
66     element = wait.until(EC.visibility_of_element_located((By.CLASS_NAME, {\cf22 'cargo'})))\par
67     \par
68     {\cf20 # Processo para mostrar mais vagas, clica o nr_pag indicado, "abrir nova p\'E1gina".}\par
69     j=0\par
70     {\cf19 while} (j < nr_pag):\par
71         {\cf19 try}:\par
72             nav.find_element(By.CSS_SELECTOR, {\cf22 '#maisVagas'}).send_keys(Keys.ENTER)\par
73             time.sleep(2)  {\cf20 # segundos}\par
74             j = j+1\par
75         {\cf19 except}:\par
76             {\cf19 break}\par
77     \par
78     {\cf20 # marca o cabe\'E7alho de cada anuncio do resultado da busca.}\par
79     lista_geral = nav.find_elements(By.CLASS_NAME,{\cf22 'informacoes-header'})\par
80 \par
81     lista_vagas = []  {\cf20 #lista que a fun\'E7\'E3o dar\'E1 como resposta}\par
82     \par
83     {\cf20 # para cada an\'FAncio da p\'E1gina completa, pegar\'E1 as seguintes informa\'E7\'F5es:}\par
84         {\cf20 #  id, t\'EDtulo, nome empresa, n\'EDvel e link da vaga }\par
85 \par
86     {\cf19 for} anuncio {\cf19 in} lista_geral:\par
87         id_vg = anuncio.find_element(By.CLASS_NAME, {\cf22 'link-detalhes-vaga'}).get_attribute({\cf22 'id'})\par
88         tit_vg = anuncio.find_element(By.CLASS_NAME, {\cf22 'cargo'}).text\par
89         empresa = anuncio.find_element(By.CLASS_NAME, {\cf22 'emprVaga'}).text\par
90         nivel_vg = anuncio.find_element(By.CLASS_NAME, {\cf22 'nivelVaga'}).text\par
91         link_vg = anuncio.find_element(By.CLASS_NAME, {\cf22 'link-detalhes-vaga'}).get_attribute({\cf22 'href'})\par
92         lista_vagas.append((id_vg, tit_vg, empresa, nivel_vg, link_vg))\par
93         \par
94     lista_vagas_vagas_com = pd.DataFrame(lista_vagas, columns=[{\cf22 'id'}, {\cf22 'titulo'},  {\cf22 'empresa'}, {\cf22 'nivel_da_vaga'}, {\cf22 'link'}])\par
95     {\cf19 print} (len(lista_vagas_vagas_com))\par
96     lista_vagas_vagas_com.to_csv({\cf22 'Lista_geral_links.csv'})\par
97     \par
98     nav.close()\par
99     \par
100     {\cf19 return} lista_vagas_vagas_com\par
101 \par
102 {\cf20 # SEGUNDA FUN\'C7AO}\par
103 \par
}
}
